<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 引入mysql-connector-java-5.1.30.jar位置 -->
	<classPathEntry
		location="C:\Users\dhsu_2019\.m2\repository\mysql\mysql-connector-java\5.1.13\mysql-connector-java-5.1.13.jar" />

	<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
		</commentGenerator>

		<!-- 数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1/kefu" userId="root"
			password="admin">
		</jdbcConnection>

		<!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer true，把JDBC DECIMAL 和 
			NUMERIC 类型解析为java.math.BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成model模型，对应的包路径，以及文件存放路径(targetProject)，targetProject可以指定具体的路径 也可以使用“MAVEN”来自动生成，这样生成的代码会在target/generatord-source目录下 -->
		<javaModelGenerator targetPackage="com.iflytek.renshou.pojo"
			targetProject="D:\git_repo\springboot-mybatis-thymeleaf\renshou\src\main\java\">
			<property name="enableSubPackages" value="false" />
			<!-- 从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--对应的mapper.xml文件 -->
		<sqlMapGenerator targetPackage="\"
			targetProject="D:\git_repo\springboot-mybatis-thymeleaf\renshou\src\main\resources\mapper">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<!-- 对应的Mapper接口类文件 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.iflytek.renshou.dao"
			targetProject="D:\git_repo\springboot-mybatis-thymeleaf\renshou\src\main\java\">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- 生成哪些表，tableName对应数据库表，domainObjectName对应实体类名 -->
		<table tableName="iat_data"
			domainObjectName="IatData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="ims_ai_data"
			domainObjectName="ImsAiData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="ims_asr_data"
			domainObjectName="ImsAsrData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="ims_tts_data"
			domainObjectName="ImsTtsData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="ism_data"
			domainObjectName="IsmData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="jianxi_data"
			domainObjectName="JianXiData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="tts_data"
			domainObjectName="TtsData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
		<table tableName="yanzhi_data"
			domainObjectName="YanZhiData" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
		</table>
	</context>
</generatorConfiguration>